  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DAAS.Application\DAAS.Application.csproj" />
    <ProjectReference Include="..\DAAS.Infrastructure\DAAS.Infrastructure.csproj" />
  </ItemGroup>

</Project>
